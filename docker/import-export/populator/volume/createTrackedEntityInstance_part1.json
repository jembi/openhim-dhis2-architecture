{
  "name": "Part 1 of Creating a DHIS2 Tracked Entity Instances",
  "endpoint": {
    "method": "POST",
    "pattern": "/trackedEntityInstance"
  },
  "transformation": {
    "input": "JSON",
    "output": "JSON"
  },
  "inputMapping": {
    "requestBody.requestBody.orgUnitID": "orgUnit",
    "requestBody.requestBody.event": "event",
    "requestBody.enrollmentDate": "enrollmentDate",
    "requestBody.incidentDate": "incidentDate",
    "lookupRequests.createTrackedEntityInstance.response.importSummaries[0].reference": "trackedEntityInstance"
  },
  "inputValidation": {
    "type": "object",
    "properties": {
      "requestBody": {
        "type": "object",
        "properties": {
          "enrollmentDate": {
            "type": "string",
            "nullable": true
          },
          "incidentDate": {
            "type": "string",
            "nullable": true
          }
        }
      },
      "lookupRequests": {
        "type": "object",
        "properties": {
          "createTrackedEntityInstance": {
            "type": "object",
            "properties": {
              "response": {
                "type": "object",
                "properties": {
                  "importSummaries": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "reference": {
                          "type": "string"
                        }
                      },
                      "required": ["reference"]
                    }
                  }
                },
                "required": ["importSummaries"]
              }
            },
            "required": ["response"]
          }
        }
      }
    }
  },
  "requests": {
    "lookup": [
      {
        "id": "createTrackedEntityInstance",
        "config": {
          "method": "GET",
          "url": "http://populator:3003/createTrackedEntityInstance",
          "headers": {
            "Content-Type": "application/json"
          },
          "params": {
            "orgUnit": {
              "path": "payload.requestBody.orgUnitID"
            },
            "name": {
              "path": "payload.requestBody.name"
            },
            "surname": {
              "path": "payload.requestBody.surname"
            },
            "rsaid": {
              "path": "payload.requestBody.rsaid"
            },
            "dateOfBirth": {
              "path": "payload.requestBody.dateOfBirth"
            }
          }
        }
      }
    ],
    "response": [
      {
        "id": "createEnrollment",
        "config": {
          "method": "POST",
          "url": "http://openhim-core:5001/populator/enrollment",
          "headers": {
            "Content-Type": "application/json"
          }
        }
      }
    ]
  }
}
