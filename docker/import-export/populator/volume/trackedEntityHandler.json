{
  "name": "Create/Update Tracked Entity (Pregnancy Tracker)",
  "endpoint": {
    "method": "POST",
    "pattern": "/trackedEntityHandler"
  },
  "transformation": {
    "input": "JSON",
    "output": "JSON"
  },
  "inputMapping": {},
  "inputValidation": {
    "type": "object",
    "properties": {
      "requestBody": {
        "type": "object",
        "properties": {
          "orgUnitID": {
            "type": "string"
          },
          "rsaid": {
            "type": "string"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "surname": {
            "type": "string",
            "nullable": true
          },
          "dateOfBirth": {
            "type": "string",
            "pattern": "^[12][90]\\d\\d\\-[01]\\d\\-[0-3]\\d$",
            "nullable": true
          },
          "event": {
            "type": "object",
            "properties": {
              "programStage": {
                "type": "string"
              }
            },
            "required": ["programStage"]
          }
        },
        "required": ["orgUnitID", "rsaid"]
      }
    }
  },
  "requests": {
    "lookup": [
      {
        "id": "getTrackedEntityInstance",
        "config": {
          "method": "GET",
          "url": "http://dhis-web:8080/api/trackedEntityInstances",
          "headers": {
            "Content-Type": "application/json",
            "Authorization": "Basic YWRtaW46ZGlzdHJpY3Q="
          },
          "params": {
            "filter": {
              "prefix": "NWRtCWw116x:EQ:",
              "path": "payload.rsaid"
            },
            "ou": {
              "path": "payload.orgUnitID"
            }
          }
        }
      }
    ],
    "response": [
      {
        "id": "trackedEntityInstanceHandler",
        "config": {
          "method": "POST",
          "url": "http://openhim-core:5001/populator/trackedEntityInstance",
          "headers": {
            "Content-Type": "application/json"
          }
        }
      }
    ]
  }
}
