{
  "name": "Birth Event",
  "endpoint": {
    "method": "POST",
    "pattern": "/birth"
  },
  "transformation": {
    "input": "JSON",
    "output": "JSON"
  },
  "inputMapping": {
    "requestBody.rsaid": "teiIdentifier",
    "lookupRequests.getOrgUnitID.organisationUnits[0].id": "orgUnit",
    "constants.program": "program",
    "constants.trackedEntityType": "trackedEntityType",
    "constants.status": "event.status",
    "constants.stage": "event.programStage",
    "transforms.enrollmentDate": {
      "key": "enrollmentDate",
      "transform": {
        "function": "dateTimeToDate"
      }
    },
    "transforms.incidentDate": {
      "key": "incidentDate",
      "transform": {
        "function": "dateTimeToDate"
      }
    },
    "transforms.rsaid": "attributes[]+",
    "transforms.name": "attributes[]+",
    "transforms.surname": "attributes[]+",
    "transforms.dateOfBirth": "attributes[]+",
    "transforms.babyDateOfBirth": "event.dataValues[]+",
    "transforms.babyWeight": "event.dataValues[]+"
  },
  "inputTransforms": {
    "rsaid": "${'attribute': 'NWRtCWw116x', 'value': requestBody.rsaid}",
    "name": "$exists(requestBody.name) ? {'attribute': 'OEjlQ9cmF2F', 'value': requestBody.name} : null",
    "surname": "$exists(requestBody.surname) ? {'attribute': 'bDUGWw9H8Uc', 'value': requestBody.surname} : null",
    "dateOfBirth": "$exists(requestBody.dateOfBirth) ? {'attribute': 'GLTZkxUqepl', 'value': requestBody.dateOfBirth} : null",
    "enrollmentDate": "$exists(requestBody.enrollmentDate) ? requestBody.enrollmentDate : $now()",
    "incidentDate": "$exists(requestBody.incidentDate) ? requestBody.incidentDate : $now()",
    "babyDateOfBirth": "$exists(requestBody.babyDateOfBirth) ? {'dataElement': 'eUDk5BaLwmd', 'value': requestBody.babyDateOfBirth} : null",
    "babyWeight": "$exists(requestBody.babyWeight) ? {'dataElement': 'c7qpjrW2Scm', 'value': requestBody.babyWeight} : null"
  },
  "inputValidation": {
    "type": "object",
    "properties": {
      "requestBody": {
        "type": "object",
        "properties": {
          "facilityCode": {
            "type": "string"
          },
          "rsaid": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "surname": {
            "type": "string"
          },
          "dateOfBirth": {
            "type": "string",
            "pattern": "^[12][90]\\d\\d\\-[01]\\d\\-[0-3]\\d$"
          },
          "enrollmentDate": {
            "type": "string"
          },
          "incidentDate": {
            "type": "string"
          },
          "babyDateOfBirth": {
            "type": "string",
            "pattern": "^[12][90]\\d\\d\\-[01]\\d\\-[0-3]\\d$"
          },
          "babyWeight": {
            "type": "string",
            "pattern": "^\\d\\.\\d{1,3}$|^\\d$"
          }
        },
        "required": [
          "rsaid",
          "facilityCode",
          "babyDateOfBirth",
          "babyWeight"
        ]
      },
      "lookupRequests": {
        "type": "object",
        "properties": {
          "getOrgUnitID": {
            "type": "object",
            "properties": {
              "organisationUnits": {
                "type": "array",
                "minItems": 1,
                "maxItems": 1,
                "items": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    }
                  },
                  "required": ["id"]
                }
              }
            },
            "required": ["organisationUnits"]
          }
        },
        "required": ["getOrgUnitID"]
      }
    }
  },
  "requests": {
    "lookup": [
      {
        "id": "getOrgUnitID",
        "config": {
          "method": "GET",
          "url": "http://facility-cache:3500/api/organisationUnits.json",
          "params": {
            "query": {
              "filter": {
                "prefix": "code:eq:",
                "path": "payload.facilityCode"
              }
            }
          }
        }
      }
    ],
    "response": [
      {
        "id": "fileQueue",
        "config": {
          "method": "POST",
          "url": "http://file-queue:4002/tracker-entity-queue",
          "headers": {
            "Content-Type": "application/json"
          }
        }
      }
    ]
  },
  "constants": {
    "trackedEntityType": "lXGgEp7UjA5",
    "program": "UrRxeuNqeco",
    "stage": "TAd4JyIaqYB",
    "status": "ACTIVE"
  }
}
